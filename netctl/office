Interface=$dev
Connection=wireless
Security=wpa-configsection
ESSID="$INSTALL_OFFICE_SSID"
WPAConfigSection=(
    'ssid="$INSTALL_OFFICE_SSID"'
    'scan_ssid=1'
    'key_mgmt=WPA-EAP'
    'pairwise=CCMP'
    'eap=PEAP'
    'phase1="peaplabel=0"'
    'phase2="auth=MSCHAPv2"'
    # echo -n 'password' | iconv -t utf16le | openssl md4
    'identity="$INSTALL_OFFICE_USERNAME"'
    'password=hash:PASSWORDHASH'
)
IP=dhcp
